        ::= { zxAnXponOnuCfgMgmt 1 }

    zxAnXponOnuExtCfgEntry OBJECT-TYPE
        SYNTAX      ZxAnXponOnuExtCfgEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "It is entry in the zxAnXponOnuExtCfgTable."
        INDEX   { zxAnXponOltIfIndex,zxAnXponOnuIfIndex }
        ::= { zxAnXponOnuExtCfgTable 1 }
        
    ZxAnXponOnuExtCfgEntry ::=
        SEQUENCE {
            zxAnXponOnuUniLoopDisablePeriod        Integer32,
            zxAnXponOnuUniLoopEnablePeriod         Integer32
        }
        
    zxAnXponOnuUniLoopDisablePeriod OBJECT-TYPE
        SYNTAX      Integer32(60..86400)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the UNI disabled period due to loop.
             ONU must keep UNI(due to loop) disabled for this period."
        DEFVAL { 60 }
        ::= { zxAnXponOnuExtCfgEntry 1 }

    zxAnXponOnuUniLoopEnablePeriod OBJECT-TYPE
        SYNTAX      Integer32(15..86400)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the UNI enabled period due to loop.
             Once UNI(due to loop) disabled period is reached, ONU re-enable 
             the disabled UNI(due to loop) for this period. Once no loop is 
             detected during this period, ONU keeps the UNI enabled,  
             else ONU disables the UNI again."
        DEFVAL { 15 }
        ::= { zxAnXponOnuExtCfgEntry 2 }

