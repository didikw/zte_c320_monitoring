    zxAnXponOnuCfmGlobalObjects   OBJECT IDENTIFIER ::= { zxAnXponOnuCfmMgmt 1}

    zxAnXponOnuCfmMepAvailableUnis OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(129))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute is a list of uni IDs with which 
             this MEP is associated. 
             The first byte represent the total number of uni ports, 
             every two bytes represent a uni port. 
             In these two bytes, the first 4 bits represent iftype and then 
             4 bits represent slot, the last 8 bits represent uni port.
             eg: IfType(4 bits)| Slot(4 bits) |Port(uni)(8bits)."
        ::= { zxAnXponOnuCfmGlobalObjects 1 }      
 
--CFM MA EXT
 
    zxAnXponOnuCfmMaTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnXponOnuCfmMaEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This managed entity models an IEEE 802.1 service defined 
             on a bridge port. A maintenance association (MA) is 
             a set of endpoints on opposite sides of a network, 
             all existing at a defined maintenance level."
        REFERENCE   "G.984.4 9.3.20 Dot1ag maintenance association"
        ::= { zxAnXponOnuCfmMgmt 2 }

    zxAnXponOnuCfmMaEntry OBJECT-TYPE
        SYNTAX      ZxAnXponOnuCfmMaEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "It is entry in the zxAnXponOnuCfmMaTable."
        INDEX   { zxAnXponOnuCfmMdId, zxAnXponOnuCfmMaId }
        ::= { zxAnXponOnuCfmMaTable 1 }
        
    ZxAnXponOnuCfmMaEntry ::=
        SEQUENCE {
            zxAnXponOnuCfmMdId                  Integer32,
            zxAnXponOnuCfmMaId                  Integer32,
            zxAnXponOnuCfmMaVlans               DisplayString,
            zxAnXponOnuCfmMaMhfCreation         INTEGER,
            zxAnXponOnuCfmMaIdPermission        INTEGER,
            zxAnXponOnuCfmMaRowStatus           RowStatus
        }
           
    zxAnXponOnuCfmMdId   OBJECT-TYPE
        SYNTAX      Integer32(1..16)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "MD ID uniquely identifies an instance of this managed entity."
        ::= { zxAnXponOnuCfmMaEntry 1 }
        
    zxAnXponOnuCfmMaId OBJECT-TYPE
        SYNTAX      Integer32(1..32)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute uniquely identifies an instance of MA."
        ::= { zxAnXponOnuCfmMaEntry 2 }
            
    zxAnXponOnuCfmMaVlans OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..64))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute is a list of up to 12 VLAN IDs with which 
             this MA is associated. 
             The first entry is understood to be the primary VLAN.
             eg: 100,15-16 means that the primary VLAN is 100, MA is associated
             with VLAN 15,16,100."
        ::= { zxAnXponOnuCfmMaEntry 3 }

    zxAnXponOnuCfmMaMhfCreation OBJECT-TYPE
        SYNTAX      INTEGER {
                    defMHFnone(1),
                    defMHFdefault(2),
                    defMHFexplicit(3),
                    defMHFdefer(4)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute determines whether the bridge creates a MHF or not, 
             under circumstances defined in IEEE 802.1ag clause 22.2.3.
             defMHFnone(1), No MHFs can be created for this VID.
             defMHFdefault(2),  MHFs can be created on this VID on any
                                Bridge port through which this VID can pass.
             defMHFexplicit(3), MHFs can be created for this VID only on
                                Bridge ports through which this VID can
                                pass, and only if a MEP is created at some
                                lower MD Level.
             defMHFdefer(4),    The creation of MHFs is determined by the
                                corresponding Maintenance Domain variable
                                (dot1agCfmMaCompMhfCreation)."
        DEFVAL { defMHFdefer }
        ::= { zxAnXponOnuCfmMaEntry 4 }

    zxAnXponOnuCfmMaSenderId OBJECT-TYPE
        SYNTAX      INTEGER {
                    sendIdNone(1),
                    sendIdChassis(2),
                    sendIdManage(3),
                    sendIdChassisManage(4),
                    sendIdDefer(5)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute determines the content of the sender ID TLV included
             in CFM messages transmitted by MP controlled by this MA.
             sendIdNone(1),     The Sender ID TLV is not to be sent.
             sendIdChassis(2),  The Chassis ID Length, Chassis ID
                                Subtype, and Chassis ID fields of  the
                                Sender ID TLV are to be sent.
             sendIdManage(3),    The Management Address Length and
                                Management Address of the Sender ID TLV
                                are to be sent.
             sendIdChassisManage(4), The Chassis ID Length, Chassis ID
                                Subtype, Chassis ID, Management Address
                                Length and Management Address fields are
                                all to be sent.
             sendIdDefer(5),    The contents of the Sender ID TLV are
                                determined by the corresponding
                                Maintenance Domain variable
                                (dot1agCfmMaCompIdPermission)."
        DEFVAL { sendIdDefer }
        ::= { zxAnXponOnuCfmMaEntry 5 }
        
    zxAnXponOnuCfmMaRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the operation of the entry."
        ::= { zxAnXponOnuCfmMaEntry 55 } 
        
