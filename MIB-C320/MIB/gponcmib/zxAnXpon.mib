-- Configuration Data Copy
--------------------------------------------------------------------------------

    zxAnXponCfgDataCopy       OBJECT IDENTIFIER ::= { zxAnXponCfgDataMgmt 1}

    zxAnXponCfgDataCopyType OBJECT-TYPE
        SYNTAX      INTEGER {
            ponPort(1),
            onu(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the type of configuration data copying.
            The copying is between PON ports or ONUs."
        ::= { zxAnXponCfgDataCopy 1 }

    zxAnXponCfgDataCopySrcIfIndex OBJECT-TYPE
        SYNTAX      ZxAnIfindex
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the source for configuration data
            copying. When 'zxAnXponCfgDataCopyType' is set to 'ponPort(1)',
            the field of interface type in ifIndex should be for a PON port.
            While 'zxAnXponCfgDataCopyType' is set to 'onu(2)', the ifIndex
            represents an ONU."
        ::= { zxAnXponCfgDataCopy 2 }

    zxAnXponCfgDataCopyDstIfIndex OBJECT-TYPE
        SYNTAX      ZxAnIfindex
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the destination for configuration data
            copying. When 'zxAnXponCfgDataCopyType' is set to 'ponPort(1)',
            the field of interface type in ifIndex should be for a PON port.
            While 'zxAnXponCfgDataCopyType' is set to 'onu(2)', the ifIndex
            represents an ONU."
        ::= { zxAnXponCfgDataCopy 3 }
        
    zxAnXponCfgDataCopyForceEnable   OBJECT-TYPE
        SYNTAX      INTEGER {
            noForce(1),
            force(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates whether configuration data will be copied
            if there are configuration data in the destination PON port or ONU.
            When 'noForce(1)' is set, the configuration data will not be copied
            to the destination PON port or ONU if it has been configured. 
            When 'force(2)' is set, the configuration data will be copied to
            the destination PON port or ONU even if it has been configured."
        DEFVAL { noForce }
        ::= { zxAnXponCfgDataCopy 4 }
        
    zxAnXponCfgDataCopyStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            notStarted(1),
            inProgress(2),
            success(3),
            failed(4)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This attribute indicates the status of configuration data
            copying."
        ::= { zxAnXponCfgDataCopy 5 }

    zxAnXponCfgDataCopyProgress OBJECT-TYPE
        SYNTAX      INTEGER(0..100)
        UNITS       "percent"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This attribute indicates the current progress of configuration
            data copying in terms of percentage."
        ::= { zxAnXponCfgDataCopy 6 }
        
